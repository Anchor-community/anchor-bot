{
  "name": "anchor-bot",
  "version": "1.0.0",
  "type": "commonjs",
  "engines": {
    "node": ">=16.11.1"
  },
  "scripts": {
    "new": "hygen guild-command add",
    "start": "node ./build/index.js",
    "dev": "ts-node ./src/index.ts",
    "lint": "eslint --fix; prettier --write src/**/*.ts",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^0.23.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ffmpeg-static": "^4.4.0",
    "husky": "^7.0.2",
    "hygen": "^6.1.0",
    "lint-staged": "^11.2.3",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "peerdependencies": {
    "@types/node": "^16.11.0"
  },
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "discord-api-types": "^0.23.1",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "libsodium-wrappers": "^0.7.9",
    "prettier": "^2.4.1",
    "sodium": "^3.0.2",
    "ytdl-core": "^4.9.1"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1"
  },
  "description": "Management bot for Anchor server",
  "main": "build/index.js",
  "repository": "git@github.com:Anchor-community/anchor-bot.git",
  "author": "Hiro Degawa<hxdegawa@gmail.com>",
  "license": "MIT"
}
